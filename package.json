  "dependencies": {
    "mkdirp-promise": "5.0.1",
    "util": "0.12.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.0",
    "@commitlint/cli": "8.2.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "commitizen": "4.0.3",
